
<% if session[:user_id] %>
  <%= button_to "Log Out", login_path, method: :delete  %>

<section class='existing-users'>
      <p>Existing Users: </p>
      <% @users.each do |user| %>
          <li><%= user.email %></li>
      <% end %>
  </section>

<% else %>

  <section>
    <%= button_to "Log In", login_path, method: :get %>
  </section>
  <br>
  <section>
    <%= button_to "Create New User", register_path, method: :get %>
  </section>

<% end %>